#!/bin/bash
#
# simple test with env to check if the output is the same

command="env"
OUTPUTFILE22="checker_output_$R"

# send commands
echo "$command" | $SHELL | head -n -1 | wc -c > $OUTPUTFILE 2> /dev/null &

# wait a little bit
$SLEEP $SLEEPSECONDS

"$command" | head -n -1 | wc -c > $OUTPUTFILE22 2> /dev/null &

# wait a little bit
$SLEEP $SLEEPSECONDS

# check the result
nmatch=`cat $OUTPUTFILE`
nmatch2=`cat $OUTPUTFILE22`
if [ $nmatch -eq $nmatch2 ]; then
	   print_ok
else
	   print_ko
fi

# clean up
stop_shell
